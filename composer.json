{
    "name": "penneo/penneo-sdk-php",
    "type": "library",
    "homepage": "https://penneo.com",
    "description": "Penneo SDK for PHP",
    "autoload": {
        "psr-4": {
            "Penneo\\SDK\\": "src/",
            "Penneo\\SDK\\Tests\\": "features/Context/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GuzzleHttp\\Tests\\": "vendor/guzzlehttp/guzzle/tests/"
        }
    },
    "require": {
        "php": ">=5.3.0",
        "guzzlehttp/guzzle": "^6.0",
        "davedevelopment/guzzle-wsse-auth-plugin": "1.*",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "behat/behat": "^3.0",
        "phpunit/phpunit": "~5.0",
        "squizlabs/php_codesniffer": "*"
    },
    "config": {
        "bin-dir": "bin"
    }
}

